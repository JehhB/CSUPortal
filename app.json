{
  "expo": {
    "name": "portal",
    "slug": "portal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#450a0a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#450a0a"
      },
      "package": "com.ecobayod.portal"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": ["expo-router", "expo-asset"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "708a67c3-4a1c-47ec-9f31-9d195ff99d64"
      }
    }
  }
}
